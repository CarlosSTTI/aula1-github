package application;

public class Matrizes2 {

	public static void main(String[] args) {
		
		int[][] notasAlunos = new int[30][4];
		
		notasAlunos[0][0] = 9;
		notasAlunos[0][1] = 8;
		notasAlunos[0][2] = 0;
		notasAlunos[0][3] = 7;
		
		notasAlunos[1][0] = 6;
		notasAlunos[1][1] = 5;
		notasAlunos[1][2] = 4;
		notasAlunos[1][3] = 4;
		
		notasAlunos[2][0] = 2;
		notasAlunos[2][1] = 2;
		notasAlunos[2][2] = 1;
		notasAlunos[2][3] = 3;
		
		for (int i=0; i<notasAlunos.length; i++) {
			for (int j=0; j<notasAlunos[i].length; j++) {
				System.out.println(notasAlunos[i][j] + " - 	");
			}
			System.out.println();
		}
	}

}
