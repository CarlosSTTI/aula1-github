package application;

import java.util.ArrayList;
import java.util.List;

public class Listas {

	public static void main(String[] args) {
		
		List <String> list = new ArrayList<>();
		
		list.add("Maria");
		list.add("Adriano");
		list.add("Carlos");
		list.add(0, "Daniel");
		
		for (String o : list) {
			System.out.println(o);
		}
		

	}

}
