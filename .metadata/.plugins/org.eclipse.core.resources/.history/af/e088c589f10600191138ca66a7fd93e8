package application;

import java.util.Scanner;

public class Tentando {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		System.out.println("Quantos alunos possui nesta sala? ");
		int n = sc.nextInt();
		double notaAlunos [] = new double [n];
		
		for (int i = 0; i<n; i++) {
		notaAlunos[i] = sc.nextDouble();
		System.out.println("Alunos" + notaAlunos[i]);
			
		}
		
	}

}
